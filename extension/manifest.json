{
"manifest_version": 2,
    "name": "Add MAL Columns",
    "description": "Exactly what it says",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "http://myanimelist.net/*"
            ],
            "js": [
                "jquery-1.12.0.min.js", "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "background": {
        "scripts": ["jquery-1.12.0.min.js"]
    }
}